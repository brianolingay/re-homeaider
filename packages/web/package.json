    "@zeit/next-less": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "babel-plugin-import": "^1.12.0",
    "cross-env": "^5.2.0",
    "less": "^3.9.0",
    "less-vars-to-js": "^1.3.0",
    "nodemon": "^1.19.0",
    "null-loader": "^3.0.0",
    "ts-node": "^8.1.0",
    "typescript": "^3.5.1"
  },
  "workspaces": {
    "nohoist": [
      "**/@types/react",
      "**/@types/react/**",
      "**/@types/react-apollo",
      "**/@types/react-apollo/**",
      "**/@types/react-dom",
      "**/@types/react-dom/**",
      "**/rimraf",
      "**/rimraf/**",
      "**/@types/next",
      "**/@types/next/**",
      "**/@zeit/next-typescript",
      "**/@zeit/next-typescript/**",
      "**/@zeit/next-css",
